---

<% item = @item %>

	<section class="detail-page-wrap clearfix">
		<div class="project-details"> 
				<ul class="icons-discipline">
				<% item.categories.each do |f| %>
				<li class="icon-<%= f %>"><%= f %></li>
				<% end %>		
			</ul>
			<h3><%= item.title %></h3>
			<p><%= item.summary %></p>
			<% if item.role %>
				<h4>Role</h4>
				<p><%= item.role %></p>
			<% end %>

			<% if item.technologyUsed %>
				<h4>Technology Used</h4>
					<p><% item.technologyUsed.each do |tech| %><%= tech %>, 
					<% end %>
					</p>
			<% end %>

			<% if item.company %>
			<h4>Company</h4>
			<p><%= item.company %></p>
			<% end %>	
			
			<% if item.cta %>
				<a class="cta" target="_blank" title="Link to <%= item.title %>" href="<%= item.cta.ctaLink %>"><%= item.cta.ctaText %></a>
			<% end %>
	</div>
	<div class="project-image-col clearfix">
		<img src="../img/fpo/responsive-fpo.png" class="img-project" srcset="
			../img/fpo/sm-responsive-fpo.png 320w, 
		  ../img/fpo/sm-responsive-fpo@2x.png 320w 2x,
			../img/fpo/responsive-fpo.png 321w, 
		  ../img/fpo/responsive-fpo@2x.png 321w 2x,
			../img/fpo/lg-responsive-fpo.png 960w, 
		  ../img/fpo/lg-responsive-fpo@2x.png 960w 2x" 
		/>
	<%= image_tag 'fpo/desktop-screenshot.jpg', :title => 'RovenBashier.com Desktop Layout', :alt => 'RovenBashier.com Desktop Layout' %>
